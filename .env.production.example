# =============================================================================
# Production Environment Variables (EXAMPLE)
# Copy this to .env.production and fill in real values.
# NEVER commit real production secrets to the repository.
# =============================================================================

# Database (RDS endpoint)
DATABASE_URL=postgresql://postgres:CHANGE_ME@your-rds-endpoint:5432/codearena
POSTGRES_PASSWORD=CHANGE_ME
POSTGRES_DB=codearena

# JWT Secret (generate with: openssl rand -hex 32)
JWT_SECRET=CHANGE_ME
JWT_EXPIRES_IN=7d

# Redis (ElastiCache endpoint)
REDIS_HOST=your-elasticache-endpoint
REDIS_PORT=6379
REDIS_URL=redis://your-elasticache-endpoint:6379

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672

# CORS Origins (your domain or public IP)
CORS_ORIGIN=https://your-domain.com

# Frontend API URLs
VITE_AUTH_API_URL=https://api.your-domain.com
VITE_BATTLE_API_URL=https://api.your-domain.com
VITE_EXECUTION_API_URL=https://api.your-domain.com
VITE_RATING_API_URL=https://api.your-domain.com
VITE_WS_URL=wss://ws.your-domain.com

# WebSocket Client URL
CLIENT_URL=https://your-domain.com

# Environment
NODE_ENV=production
